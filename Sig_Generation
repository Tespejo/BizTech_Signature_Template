	   <script type="text/javascript">

		   function getParameterByName(name, url) {
				if (!url) url = window.location.href;
				name = name.replace(/[\[\]]/g, "\\$&");
				var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
					results = regex.exec(url);
				if (!results) return null;
				if (!results[2]) return '';
				return decodeURIComponent(results[2].replace(/\+/g, " "));
			}
		    var teleId = getParameterByName('teleId');
		    var mobileId = getParameterByName('mobileId');
		    var addId = getParameterByName('addId');
		    var empName = getParameterByName('empName');
		    var empTitle = getParameterByName('empTitle');
        var empEmail = getParameterByName('empEmail');
        
		    document.getElementById('teleId').innerHTML= teleId;
		    document.getElementById('mobileId').innerHTML= mobileId;
		    document.getElementById('addId').innerHTML= addId;
		    document.getElementById('empName').innerHTML= empName;
		    document.getElementById('empTitle').innerHTML= empTitle;
        document.getElementById('empEmail').innerHTML= empEmail;

      
        var dynamicEmail = document.getElementById('dynEmpEmail');
        dynamicEmaila = dynamicEmail.setAttribute('href','mailto:' + empEmail);
        

// REPLY SIGNATURE DATA SETTING //
    
