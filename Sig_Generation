        document.getElementById('replyEmpTitle').innerHTML= replyEmpTitle;
        document.getElementById('replyEmpMobile').innerHTML= replyEmpMobile;
        document.getElementById('replyEmpEmail').innerHTML= replyEmpEmail;

        var BdynamicEmail = document.getElementById('BdynamicEmail');
        BdynamicEmaila = BdynamicEmail.setAttribute('href','mailto:' + replyEmpEmail);
}
       






